{
  "expo": {
    "name": "seoul-smart-app",
    "slug": "seoul-smart-app",
    "privacy": "public",
    "sdkVersion": "34.0.0",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/logo.png",
    "splash": {
      "backgroundColor": "#0287cb"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "packagerOpts": {
      "sourceExts": ["jsx", "js"],
      "config": "metro.config.js"
    }
  }
}
